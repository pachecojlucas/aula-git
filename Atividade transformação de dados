      "cell_type": "code",
      "source": [
        "# utilizando o apply"
      ],
      "metadata": {
        "id": "tNorYZlpWZng"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def bonus_trat(row):\n",
        "  if row['start_year'] < 2010 and row['gender_trat'] == 'Female':\n",
        "    return row['Bonus %'] + 8\n",
        "  elif row['start_year'] < 2010 and row['gender_trat'] == 'Male':\n",
        "    return row['Bonus %'] + 5\n",
        "  else:\n",
        "    return row['Bonus %']"
      ],
      "metadata": {
        "id": "3S4k0YeBWcTU"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# df[[colunas]].apply(func, axis=0 ou 1)\n",
        "\n",
        "df['apply_bonus_trat'] = df.apply(bonus_trat, axis=1)"
      ],
      "metadata": {
        "id": "B0dcq2yhWcqS"
      },
      "execution_count": null,
      "outputs": []
    },
